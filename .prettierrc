{
  "plugins": ["@ianvs/prettier-plugin-sort-imports", "prettier-plugin-tailwindcss"],
  "trailingComma": "es5",
  "tabWidth": 2,
  "semi": true,
  "singleQuote": false,
  "printWidth": 125,
  "endOfLine": "crlf",
  "importOrder": [
    "server-only",
    "",
    "^react$",
    "^next/.*$",
    "sonner",
    "<THIRD_PARTY_MODULES>",
    "^[./]",
    "<TYPES>",
    "^(?!.*[.]css$)[./].*$",
    ".css$"
  ]
}
